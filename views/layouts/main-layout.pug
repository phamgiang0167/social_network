include ../mixins/mixins

<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{pageTitle}
        link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.5.11/cropper.min.css" )
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css', integrity='sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh', crossorigin='anonymous')
        link(rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css")
        link(rel="stylesheet", href="/stylesheets/style.css")
        link(href="https://kit-pro.fontawesome.com/releases/v5.15.3/css/pro.min.css" rel="stylesheet")
    body
        script.
            var userLoggedIn =!{userLoggedInJs}
       
        .container-fluid
            .row.main
                .topnav
                    label.btn.nav-btn(for="navCheckFaculty")
                        i.far.fa-bars
                    a.btn(href="/")
                        i.far.fa-home
                    a.btn(href="/search")
                        i.far.fa-search
                    a.btn(href="/notification")
                        i.far.fa-bell
                    a.btn(href="/messenger")
                        i.fal.fa-comments
                    a.btn(href="/profile")
                        i.far.fa-user
                    a.btn(href="/auth/logout")
                        i.far.fa-sign-out-alt
                    label.btn.nav-btn(for="navCheckNotification")
                        i.far.fa-bars
                .facultyContainer
                    .navPc
                        .titleContainer 
                            h1 Categories
                        .listFacultyContainer
                            #accordion(style="width: 100%")
                                .card
                                    #headingTwo
                                        ul.list-group 
                                            li.list-group-item.item(data-toggle='collapse' data-target='#collapseTwo' aria-expanded='false' aria-controls='collapseTwo')
                                                .item-container
                                                    img(src='/images/faculty.png')
                                                    |&nbsp&nbsp
                                                    span Faculties
                                    #collapseTwo.collapse(aria-labelledby='headingTwo' data-parent='#accordion')
                                        .card-body.listFaculty
                                            
                                                
                    input.navInputFaculty#navCheckFaculty(type="checkbox" hidden)
                    label.overPlay(for="navCheckFaculty")
                    
                    .navMobileFaculy
                        label.nav-closeFaculty(for="navCheckFaculty")
                            i.fas.fa-times
                        .titleContainer 
                            h1 Categories
                        .listFacultyContainer
                            #accordion(style="width: 100%")
                                .card
                                    #headingTwo
                                        ul.list-group
                                            li.list-group-item.item(data-toggle='collapse' data-target='#collapseTwo' aria-expanded='false' aria-controls='collapseTwo')
                                                .item-container
                                                    img(src='/images/faculty.png')
                                                    |&nbsp&nbsp
                                                    span Offices
                                    #collapseTwo.collapse(aria-labelledby='headingTwo' data-parent='#accordion')
                                        .card-body.listFaculty
                                            
                                                
                .mainSectionContainer
                    
                    .titleContainer 
                        h1 #{pageTitle}
                        block headerButton
                    block content
                .notificationContainer
                    .navPc
                        .titleContainer
                            h1 Notification
                            span
                                a.viewAll view all
                        .listNotificationContainer
                            .list-group.listNotification
                        .paginationContainer
                            nav.mx-auto 
                                ul.pagination    
                    input.navInputNotification#navCheckNotification(type="checkbox" hidden)
                    label.overPlay(for="navCheckNotification")
                    .navMobileNotification
                        label.nav-closeNotification(for="navCheckNotification")
                            i.fas.fa-times
                        .titleContainer
                            h1 Notification
                        .list-group.listNotification
                        .paginationContainer
                            nav.mx-auto 
                                ul.pagination
                                
                                
                        
                        
                        
        script(src='//cdn.jsdelivr.net/npm/sweetalert2@10')
        script(src="https://unpkg.com/sweetalert/dist/sweetalert.min.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.0.1/socket.io.js")
        script(src='https://code.jquery.com/jquery-3.5.1.min.js', integrity='sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=', crossorigin='anonymous')
        script(src="https://cdnjs.cloudflare.com/ajax/libs/cropperjs/1.5.11/cropper.min.js")
        script(src='https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js', integrity='sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo', crossorigin='anonymous')
        script(src='https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js', integrity='sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6', crossorigin='anonymous')
        script(src="/javascripts/main.js")
        script(src="/javascripts/socketioClient.js")
        block scripts
    
        
        
        
        